#include<stdio.h>
main()
{
	int marks=0;
	printf("Enter your marks:- ");
	scanf("%d",&marks);
	if(marks>=85)
	{
	printf("GRADE A");
    }
	else if(marks>=85 && marks==100)
	{
	printf("GRADE A");
	}
	if(marks>=70 && marks<=84)
	{
	printf("GRADE B");
	}
	if(marks>=55 && marks<=69)
	{
	printf("GRADE C");	
	}
	if(marks>=40 && marks<=54)
	{
	printf("GRADE D");	
	}
	if(marks<40)
	{
	printf("GRADE F");	
	}
}
